funcion factorial(x){
	si(x<=1){
		retornar 1;
	}sino{
		retornar x*factorial(x- 1);
		imprimir("ve ve ve");
	}
}


funcion fibonacci(n){
        si (n<=1){
            retornar 1;
        }sino {
           retornar fibonacci(n-1) + fibonacci(n-2);
        }
}


funcion fibonacci(n,m){
       retornar (n+m);
}



imprimir(fibonacci(10));
imprimir(fibonacci(10,20));




